{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "env": {
        "SUPABASE_URL": "",
        "SUPABASE_ANON_KEY": "",
        "NEXT_PUBLIC_SUPABASE_URL": "",
        "NEXT_PUBLIC_SUPABASE_ANON_KEY": ""
    },
    "rewrites": [
        {
            "source": "/health",
            "destination": "/health"
        },
        {
            "source": "/actors/(.*)",
            "destination": "/actors/$1"
        },
        {
            "source": "/authors/(.*)",
            "destination": "/authors/$1"
        },
        {
            "source": "/histories/(.*)",
            "destination": "/histories/$1"
        }
    ],
    "routes": [
        {
            "src": "/health",
            "dest": "/health"
        },
        {
            "src": "/actors/(.*)",
            "dest": "/actors/$1"
        },
        {
            "src": "/authors/(.*)",
            "dest": "/authors/$1"
        },
        {
            "src": "/histories/(.*)",
            "dest": "/histories/$1"
        }
    ]
  
}